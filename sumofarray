#include<stdio.h>
#include<conio.h>
   void main()
     {
     int i,n,a[10],sum=0;
      clrscr();
	printf("Enter the number of elements \n");
	scanf("%d",&n);
	printf("Enter the elements \n");
	for(i=0;i<n;i++)
	 {
	 printf("a[%d]=",i);
	 scanf("%d",&a[i]);
	 sum=sum+a[i];
	 }
	 printf("\n sum of array elements %d",sum);
	 getch();
	 }
